      <!--POST-->
      <div class="post">
        <div class="the_post_content">
          <div class="profile-picture">
            <img
              src="https://i.pinimg.com/originals/a6/58/32/a65832155622ac173337874f02b218fb.png"
              alt=""
            />
            <h3>
              <a href="#">test_someone</a>
            </h3>
          </div>
  
